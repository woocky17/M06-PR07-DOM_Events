{
    "manifest_version": 3,
    "name": "DOM Modifier Amazon",
    "version": "1.0",
    "description": "Modifica el DOM de Amazon",
    "permissions": [
        "scripting",
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "https://www.amazon.es/gp/bestsellers/electronics/"
    ],
    "background": {
        "service_worker": "amazon.js"
    },
    "action": {
        "default_popup": "index.html"
    }
}